draw_line_width(X + scaledBorderWidth/2, Y+scaledHeight - scaledBorderWidth, X + scaledBorderWidth/2, Y + scaledBorderWidth, scaledBorderWidth);

//Returning to normal drawing settings
draw_set_alpha(1);
draw_set_color(c_white);

</string>
