          <argument>
            <kind>1</kind>
            <string>///Drawing Self
var scale = obj_master_UI.scale;

var X = x * scale;
var Y = y * scale;
var scaledWidth = width * scale;
var scaledHeight = height * scale;
var scaledBlockWidth = blockWidth * scale;
var scaledBlockHeight = blockHeight * scale;
var scaledBarWidth = barWidth * scale;

//Bar
draw_set_color(displayBarColor);
draw_set_alpha(barAlpha);
var barY = Y + scaledHeight/2;
draw_line_width(X, barY, X + scaledWidth, barY, scaledBarWidth);

//Block
var totalRange = maxValue - minValue;
var progression = value - minValue;
var percentage = progression / totalRange;

var blockX = X+scaledBlockWidth/2 + (scaledWidth - scaledBlockWidth) * percentage;
var blockY = Y + scaledHeight/2;
draw_set_color(displayBlockColor);
draw_set_alpha(blockAlpha);
draw_rectangle(blockX - scaledBlockWidth/2, blockY-scaledBlockHeight/2, blockX + scaledBlockWidth/2, blockY+scaledBlockHeight/2, false);

</string>
