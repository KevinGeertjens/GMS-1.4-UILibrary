            <string>///Oscillating Cursor
if(oscillateCursor &amp;&amp; keyboard_check(vk_anykey))
{
    showCursor = true;
    cursorTimer = cursorTimerMax;
}
else
{
    if(oscillateCursor)
    {
        cursorTimer -= 1*obj_deltatime.delta;
        if(cursorTimer &lt;= 0)
        {
            cursorTimer = cursorTimerMax;
            showCursor = !showCursor;
        }
    }
    else if(showCursor)
    {
        showCursor = false;
    }
}
